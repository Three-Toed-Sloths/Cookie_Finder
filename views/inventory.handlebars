<div class='content'>

    <div class='container'>
        <h1 id="signUpHead">Seller Sign-Up</h1>
        <div class='card'>

            <div class='row'>
                <div class='col s1'></div>
                <div class='col s12 m10'>
                    <div class='input-field col s12' id='cookies-selector'>
                        <select multiple class='icons'>
                            <option class='cookies-option' value=''disabled selected>Add Cookies</option>
                            {{#each productArr}}
                                {{> storeFront/productList}}
                            {{/each}}
                        </select>
                    </div>
                </div>
                <div class='col s1'></div>
            </div>
        <div class="row">
            <div class="input-field col s10 offset-s1">
                <input placeholder="Email" id="newSellerEmail" type="email" class="validate">
            </div>
        </div>
            <div class='row'>
                <div class='col s1'></div>
                <form class='col s12 m5'>
                    <div class='input-field'>
                        <i class='material-icons prefix'>account_circle</i>
                        <input id='newSellerName' type='text'>
                        <label for='newSellerName'>Name</label>
                    </div>
                </form>
                    <div class='input-field col s12 m5'>
                        <i class="fas fa-road prefix"></i>
                        <input id='street-address' type='text'>
                        <label for='street-address'>Street Address</label>
                    </div>
                <div class='col s1'></div>
            </div>

            <div class='row'>
                <div class='col s1'></div>
                <div class='input-field col s12 m5'>
                    <i class='material-icons prefix'>domain</i>
                    <input id='city' type='text'>
                    <label for='city'>City</label>
                </div>
                <div class='input-field col s6 m2'>
                    <select class="browser-default" id='stateSelect'>
                        <option value="" disabled selected>State</option>
                        <option value="AL">AL</option>
                        <option value="AK">AK</option>
                        <option value="AR">AR</option>
                        <option value="AZ">AZ</option>
                        <option value="CA">CA</option>
                        <option value="CO">CO</option>
                        <option value="CT">CT</option>
                        <option value="DC">DC</option>
                        <option value="DE">DE</option>
                        <option value="FL">FL</option>
                        <option value="GA">GA</option>
                        <option value="HI">HI</option>
                        <option value="IA">IA</option>
                        <option value="ID">ID</option>
                        <option value="IL">IL</option>
                        <option value="IN">IN</option>
                        <option value="KS">KS</option>
                        <option value="KY">KY</option>
                        <option value="LA">LA</option>
                        <option value="MA">MA</option>
                        <option value="MD">MD</option>
                        <option value="ME">ME</option>
                        <option value="MI">MI</option>
                        <option value="MN">MN</option>
                        <option value="MO">MO</option>
                        <option value="MS">MS</option>
                        <option value="MT">MT</option>
                        <option value="NC">NC</option>
                        <option value="NE">NE</option>
                        <option value="NH">NH</option>
                        <option value="NJ">NJ</option>
                        <option value="NM">NM</option>
                        <option value="NV">NV</option>
                        <option value="NY">NY</option>
                        <option value="ND">ND</option>
                        <option value="OH">OH</option>
                        <option value="OK">OK</option>
                        <option value="OR">OR</option>
                        <option value="PA">PA</option>
                        <option value="RI">RI</option>
                        <option value="SC">SC</option>
                        <option value="SD">SD</option>
                        <option value="TN">TN</option>
                        <option value="TX">TX</option>
                        <option value="UT">UT</option>
                        <option value="VT">VT</option>
                        <option value="VA">VA</option>
                        <option value="WA">WA</option>
                        <option value="WI">WI</option>
                        <option value="WV">WV</option>
                        <option value="WY">WY</option>
                    </select>
                </div>
                <div class='input-field col s6 m3'>
                    <input id='zip-code' type='text' data-length='5'>
                    <label for='zip-code'>Zip Code</label>
                </div>
                <div class='col s1'></div>
            </div>

            <div class='row'>
                <div class='col s1'></div>
                <div class='input-field col s12 m4'>
                    <i class='material-icons prefix'>event</i>
                    <input type='text' id='date' class='datepicker'>
                    <label for='date'>Date</label>
                </div>
                <div class='input-field col s12 m3'>
                    <i class='material-icons prefix'>access_time</i>
                    <input type='text' id='start' class='timepicker'>
                    <label for='start'>From</label>
                </div>
                <div class='input-field col s12 m3'>
                    <i class='material-icons prefix'>access_alarm</i>
                    <input type='text' id='end' class='timepicker'>
                    <label for='end'>To</label>
                </div>
                <div class='col s1'></div>
            </div>
        
            <a id="submit-seller" href="/signup/thankyou" class='green btn waves-effect waves-light'>Submit
                <i class="material-icons left">send</i>
            </a>

        </div>
    


    </div>
</div>


